{
	"name": "tutpick",
	"version": "0.0.0",
	"private": true,
	"author": {
		"name": "Shamim Hossain",
		"email": "contact@shamscorner.com"
	},
	"type": "module",
	"scripts": {
		"dev": "npm-run-all --parallel vite:dev typesafe-i18n",
		"vite:dev": "vite dev --host --port 5173",
		"build": "vite build",
		"start": "node build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write . && eslint . --fix",
		"configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
		"typesafe-i18n": "typesafe-i18n",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"gen-supabase-types:local": "npx supabase gen types --lang=typescript --local > src/lib/auth/database.types.ts",
		"gen-supabase-types:remote": "npx supabase gen types --lang=typescript --project-id \"livhljteejssbhddbsnd\" --schema public > src/lib/auth/database.types.ts",
		"listen-stripe": "stripe listen --forward-to localhost:5173/api/webhooks/stripe",
		"trigger-stripe-payment": "stripe trigger payment_intent.succeeded"
	},
	"devDependencies": {
		"@csstools/postcss-media-minmax": "^2.0.1",
		"@playwright/test": "^1.46.1",
		"@shamscorner/config-eslint": "^0.0.3",
		"@shamscorner/config-tailwindcss": "^0.0.4",
		"@shamscorner/shared": "^0.0.2",
		"@shamscorner/svelte-shared": "^0.0.2",
		"@shamscorner/svelte-ui": "^0.0.2",
		"@sveltejs/adapter-auto": "^3.2.4",
		"@sveltejs/adapter-vercel": "^5.4.3",
		"@sveltejs/kit": "^2.5.24",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.14",
		"@types/gtag.js": "^0.0.20",
		"@zerodevx/svelte-img": "^2.1.0",
		"autoprefixer": "^10.4.20",
		"fontaine": "^0.5.0",
		"lottie-web": "^5.12.2",
		"lucia": "^3.2.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.41",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"resend": "^4.0.0",
		"supabase": ">=1.190.0",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.5",
		"sveltekit-superforms": "^2.17.0",
		"tailwindcss": "^3.4.10",
		"tslib": "^2.6.3",
		"typesafe-i18n": "^5.26.2",
		"typescript": "^5.5.4",
		"vite": "^5.4.2",
		"vitest": "^2.0.5",
		"zod": "^3.23.8"
	},
	"dependencies": {
		"@builder.io/partytown": "^0.10.2",
		"@supabase/ssr": "^0.5.0",
		"@supabase/supabase-js": "^2.45.1",
		"arctic": "^1.9.2",
		"bits-ui": "^0.21.13",
		"date-fns": "^3.6.0",
		"dotenv": "^16.4.5",
		"formsnap": "^1.0.1",
		"lucide-svelte": "^0.429.0",
		"mode-watcher": "^0.4.1",
		"sharp": "^0.33.5",
		"stripe": "^16.8.0",
		"svelte-headless-table": "^0.18.2",
		"svelte-sonner": "^0.3.27",
		"tailwind-variants": "^0.2.1"
	}
}
